<view class="content"><view class="row b-b"><text class="tit">收货人</text><input class="input" type="text" placeholder="请填写收货人姓名" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','realname','$event',[]],['addressData']]]]]}}" value="{{addressData.realname}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">手机号码</text><input class="input" type="number" placeholder="请填写收货人手机号码" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','mobile','$event',[]],['addressData']]]]]}}" value="{{addressData.mobile}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">所在地区</text><text data-event-opts="{{[['tap',[['chooseLocation',['$event']]]]]}}" class="input" bindtap="__e">{{''+addressData.addressName+''}}</text><text class="yticon icon-shouhuodizhi"></text></view><view class="row b-b"><text class="tit">详细地址</text><input class="input" type="text" placeholder="街道楼牌号等" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','area','$event',[]],['addressData']]]]]}}" value="{{addressData.area}}" bindinput="__e"/></view><view class="row default-row"><text class="tit">设为默认</text><switch checked="{{addressData.defaule}}" color="#24BC47" data-event-opts="{{[['change',[['switchChange',['$event']]]]]}}" bindchange="__e"></switch></view><view class="bottomFixedBtn"><button class="myMoneyGoBtn yhqAlt-btn buy-now-btn" type="default" data-event-opts="{{[['tap',[['confirm',['$event']]]]]}}" bindtap="__e">提交</button></view></view>